$(document).ready(function(){

  $.ajax({
		type: "GET",
		url: "../proposal/show",
		data: 'project='+2,
		success: function( data, textStatus, jQxhr ){
			alert("Success");
      $data=jQuery.parseJSON(data);

			if($data.length>0){
        $('.AnimatedLoader').hide();
      }
      else{
        alert("ok");
      }

		},
		error: function( jqXhr, textStatus, errorThrown ){
			alert( errorThrown );
		}
	});




});
